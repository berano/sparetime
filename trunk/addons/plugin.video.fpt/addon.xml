<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.fpt"
       name="Fastpasstv"
       version="1.0"
       provider-name="nixa">
    <requires>
        <import addon="xbmc.python" version="1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Fastpasstv</summary>
        <description>This XBMC plugin enables streaming tv shows & movies videos (divxden & wisevid) from http://wwww.fastpasstv.com</description>
    </extension>
</addon>

