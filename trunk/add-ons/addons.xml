<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.fpt"
       name="Fastpasstv"
       version="1.0"
       provider-name="nixa">
    <requires>
        <import addon="xbmc.python" version="1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Fastpasstv</summary>
        <description>This XBMC plugin enables streaming tv shows & movies videos (divxden & wisevid) from http://wwww.fastpasstv.com</description>
    </extension>
</addon>

<addon id="repository.googlecode.sparetime" name="nixa addons" version="1.0" provider-name="nixa">
	<extension point="xbmc.addon.repository" name="nixa add-on repository">
		<info compressed="false">http://sparetime.googlecode.com/svn/add-ons/addons.xml</info>
		<checksum></checksum>
		<datadir zip="false">http://sparetime.googlecode.com/svn/add-ons</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Addons from http://sparetime.googlecode.com</summary>
		<description>Download and install addons from nixa repository.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>